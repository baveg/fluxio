{
  "name": "fluxio",
  "version": "0.1.0",
  "type": "module",
  "description": "Functional utility library for TypeScript",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./src/index.ts",
    "./*": "./src/*.ts"
  },
  "files": [
    "dist",
    "src",
    "README.md"
  ],
  "scripts": {
    "build": "tsup",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "functional",
    "utility",
    "typescript",
    "color",
    "number"
  ],
  "author": "baveg",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/baveg/fluxio.git"
  },
  "devDependencies": {
    "preact": "^10.26.9",
    "tsup": "^8.0.0",
    "typescript": "^5.0.0",
    "vitest": "^4.0.18"
  }
}
